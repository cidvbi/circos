<plots>
    {{#plots}}
    <plot>
        show = yes
        type = {{type}}
        file = {{file}}
        layers = 1
        margin = 0.02u

        thickness = {{thickness}}
        padding = 1
        orientation = in

        stroke_thickness = 0.1
        stroke_color = {{color}}
        color = {{color}}
        r0 = {{r0}}
        r1 = {{r1}}
    </plot>

    {{/plots}}
</plots>